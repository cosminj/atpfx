<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.google.code.atpfx">
	<!--
		if the package attribute is not set to hibernate-mapping tag, you need
		to qualify the class
	-->
	<class name="com.google.code.atpfx.data.Tick" table="TICKS">
		<id name="id" type="long" column="ID">
			<generator class="sequence">
				<param name="sequence">TICKS_SEQ</param>
			</generator>
		</id>
		<property name="pairId" column="PAIR_ID" />
		<property name="buy" />
		<property name="sell" />
		<property name="timestamp" />
		<property name="signalProviderId" column="SIGNALPROVIDER_ID" />
	</class>
</hibernate-mapping>